PORT=3000
SECRET_KEY=my-secret

# Supabase PostgreSQL
DATABASE_URL=postgresql://postgres:[PASSWORD]@db.[PROJECT_REF].supabase.co:5432/postgres
DB_SSL=true

# Supabase JWT
SUPABASE_JWKS_URL=https://[PROJECT_REF].supabase.co/auth/v1/.well-known/jwks.json
REQUIRE_AUTH=true

IP_SALT=random_value
SHOW_CLIENT_LOGS=false
SHOW_ALL_REQUEST_LOGS=false
GROQ_API_KEY="your_groq_api_key"
CEREBRAS_API_KEY="your_cerebras_api_key"

# S3/MinIO Configuration
BLOB_STORAGE_BUCKET=ito-blob-storage
TIMING_BUCKET=ito-timing-storage
AWS_REGION=us-east-1
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin
S3_FORCE_PATH_STYLE=true

# Stripe configurations
STRIPE_SECRET_KEY=sk_test_xxx
STRIPE_WEBHOOK_SECRET=whsec_xxx
STRIPE_PRICE_ID=price_xxx
APP_PROTOCOL=ito-dev
STRIPE_PUBLIC_BASE_URL=http://localhost:3000

# Free mode (disables Stripe, all users get pro features)
FREE_MODE=false
